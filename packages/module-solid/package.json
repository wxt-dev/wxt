{
  "name": "@wxt-dev/module-solid",
  "description": "WXT module to enable SolidJS support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wxt-dev/wxt.git",
    "directory": "packages/module-solid"
  },
  "homepage": "https://github.com/wxt-dev/wxt/blob/main/packages/module-solid/README.md",
  "keywords": [
    "wxt",
    "module",
    "solidjs"
  ],
  "author": {
    "name": "Aaron Klinker",
    "email": "aaronklinker1+wxt@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/wxt-dev",
  "version": "1.1.4",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "wxt",
    "check": "pnpm build && check",
    "build": "buildc -- unbuild",
    "prepare": "buildc --deps-only -- wxt prepare"
  },
  "peerDependencies": {
    "wxt": ">=0.19.16"
  },
  "dependencies": {
    "vite-plugin-solid": "^2.11.8"
  },
  "devDependencies": {
    "@aklinker1/check": "^2.1.2",
    "publint": "^0.3.12",
    "solid-js": "^1.9.9",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "wxt": "workspace:*"
  }
}
