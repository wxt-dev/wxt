{
  "name": "@wxt-dev/module-vue",
  "description": "WXT module to enable Vue support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wxt-dev/wxt.git",
    "directory": "packages/module-vue"
  },
  "homepage": "https://github.com/wxt-dev/wxt/blob/main/packages/module-vue/README.md",
  "keywords": [
    "wxt",
    "module",
    "vue"
  ],
  "author": {
    "name": "Aaron Klinker",
    "email": "aaronklinker1+wxt@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/wxt-dev",
  "version": "1.0.3",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "buildc -- tsdown",
    "check": "bun run build && check",
    "test": "echo 'noop'",
    "test:coverage": "echo 'noop'"
  },
  "peerDependencies": {
    "wxt": ">=0.19.16"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.2.3 || ^6.0.0"
  },
  "devDependencies": {
    "@aklinker1/buildc": "^1.1.6",
    "publint": "^0.3.16",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "wxt": "workspace:*"
  }
}
