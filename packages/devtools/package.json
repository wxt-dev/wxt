{
  "name": "@wxt-dev/devtools",
  "description": "Integrated devtools to make WXT development easier",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "check": "check",
    "dev": "buildc --deps-only -- wxt",
    "dev:firefox": "buildc --deps-only -- wxt -b firefox",
    "build": "buildc -- unbuild",
    "postinstall": "buildc --deps-only -- wxt prepare"
  },
  "devDependencies": {
    "@aklinker1/check": "^1.3.1",
    "@types/chrome": "^0.0.268",
    "@vitejs/plugin-vue": "^5.0.5",
    "@wxt-dev/module-vue": "workspace:*",
    "unbuild": "^2.0.0",
    "unocss": "^0.61.0",
    "vite": "^5.3.0",
    "vue": "^3.4.29",
    "vue-tsc": "^2.0.21",
    "wxt": "workspace:*"
  },
  "peerDependencies": {
    "wxt": ">=0.18.6"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.mts",
      "default": "./dist/plugin/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "buildc": {
    "include": [
      "src/**",
      "package.json",
      "build.config.ts",
      "tsconfig.json"
    ]
  }
}
