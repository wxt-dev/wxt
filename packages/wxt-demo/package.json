{
  "name": "wxt-demo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "buildc deps && wxt",
    "build": "buildc deps && wxt build",
    "build:all": "buildc deps && pnpm run --reporter-hide-prefix /^build:all:.*/",
    "build:all:chrome-mv3": "wxt build",
    "build:all:chrome-mv2": "wxt build --mv2",
    "build:all:firefox-mv3": "wxt build -b firefox --mv3",
    "build:all:firefox-mv2": "wxt build -b firefox",
    "test": "buildc deps && vitest",
    "zip": "buildc deps && wxt zip",
    "check": "buildc deps && check",
    "postinstall": "buildc deps && wxt prepare"
  },
  "dependencies": {
    "@wxt-dev/i18n": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@wxt-dev/auto-icons": "workspace:*",
    "@wxt-dev/unocss": "workspace:*",
    "sass": "catalog:",
    "typescript": "catalog:",
    "unocss": "catalog:",
    "vitest": "catalog:",
    "vitest-plugin-random-seed": "catalog:",
    "wxt": "workspace:*"
  },
  "buildc": {
    "cache": false
  }
}
