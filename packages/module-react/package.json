{
  "name": "@wxt-dev/module-react",
  "description": "WXT module to enable React support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wxt-dev/wxt.git",
    "directory": "packages/module-react"
  },
  "homepage": "https://github.com/wxt-dev/wxt/blob/main/packages/module-react/README.md",
  "keywords": [
    "wxt",
    "module",
    "react"
  ],
  "author": {
    "name": "Aaron Klinker",
    "email": "aaronklinker1+wxt@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/wxt-dev",
  "version": "1.1.5",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "wxt",
    "check": "pnpm build && check",
    "build": "buildc -- tsdown",
    "prepare": "buildc --deps-only -- wxt prepare"
  },
  "peerDependencies": {
    "wxt": ">=0.19.16"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.4.1 || ^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "publint": "^0.3.17",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "typescript": "^5.9.3",
    "wxt": "workspace:*"
  }
}
