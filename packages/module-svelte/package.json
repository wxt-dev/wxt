{
  "name": "@wxt-dev/module-svelte",
  "description": "WXT module to enable Svelte support",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wxt-dev/wxt.git",
    "directory": "packages/module-svelte"
  },
  "homepage": "https://github.com/wxt-dev/wxt/blob/main/packages/module-svelte/README.md",
  "keywords": [
    "wxt",
    "module",
    "svelte"
  ],
  "author": {
    "name": "Aaron Klinker",
    "email": "aaronklinker1+wxt@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/wxt-dev",
  "version": "2.0.4",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "buildc -- unbuild",
    "check": "pnpm build && check"
  },
  "peerDependencies": {
    "wxt": ">=0.18.6",
    "svelte": ">=5"
  },
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@aklinker1/check": "^2.1.2",
    "publint": "^0.3.12",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "wxt": "workspace:*"
  }
}
