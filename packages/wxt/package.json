    "linkedom": "catalog:",
    "magicast": "catalog:",
    "minimatch": "catalog:",
    "nano-spawn": "catalog:",
    "normalize-path": "catalog:",
    "nypm": "catalog:",
    "ohash": "catalog:",
    "open": "catalog:",
