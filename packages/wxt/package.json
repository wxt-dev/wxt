  "funding": "https://github.com/sponsors/wxt-dev",
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "wxt": "./bin/wxt.mjs",
    "wxt-publish-extension": "./bin/wxt-publish-extension.cjs"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
