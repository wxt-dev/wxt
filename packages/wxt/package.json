    "ohash": "^2.0.11",
    "open": "^10.1.2",
    "ora": "^8.2.0",
    "perfect-debounce": "^1.0.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "publish-browser-extension": "^2.3.0 || ^3.0.0",
    "scule": "^1.3.0",
